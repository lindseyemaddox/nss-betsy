
@font-face
  font-family 'betsy'
  src url(icomoon-font-path + '/betsy.ttf?l46yfz') format('truetype'), url(icomoon-font-path + '/betsy.woff?l46yfz') format('woff'), url(icomoon-font-path + '/betsy.svg?l46yfz#betsy') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'betsy' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-star-empty
  &:before
    content icon-star-empty

.icon-star-half
  &:before
    content icon-star-half

.icon-star-full
  &:before
    content icon-star-full

